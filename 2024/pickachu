import turtle as t
t.shape("turtle")
t.speed(500)
t.up()
t.goto(0,175)
t.down()
t.begin_fill()
t.fillcolor("yellow")
t.circle(100)
t.end_fill()
t.up()
t.goto(0,375)
t.fd(40)
t.down()
t.rt(135)
t.fd(10)
t.rt(180)
t.begin_fill()
t.fillcolor("yellow")
t.fd(120)
t.rt(45)
t.fd(60)
t.rt(105)
t.fd(60)
t.rt(45)
t.fd(120)
t.end_fill()
t.rt(180)
t.fd(120)
t.lt(90)
t.begin_fill()
t.fillcolor("black")
t.fd(40)
t.rt(90)
t.fd(50)
t.end_fill()
t.exitonclick()